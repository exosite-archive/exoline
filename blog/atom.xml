---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Exoline</title>
 <link href="http://exosite.github.io/exoline/blog/atom.xml" rel="self"/>
 <link href="http://exosite.github.io/exoline/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://exosite.github.io/exoline/</id>
 <author>
   <name>Dan Weaver</name>
   <email>danweaver@exosite.com</email>
 </author>

 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://exosite.github.io/exoline{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://exosite.github.io/exoline{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
